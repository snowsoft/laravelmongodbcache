{
    "name": "snowsoft/laravelmongodbcache",
    "description": "A MongoDB cache driver for Laravel",
    "homepage": "https://github.com/snowsoft/laravelmongodbcache",
    "type": "library",
    "require": {
        "php": ">=7.3.0",
        "jenssegers/mongodb": "*",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "snowsoft",
            "email": "abdulkadirkar@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nlk\\Mongodb\\Cache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nlk\\Mongodb\\Cache\\MongoCacheServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
